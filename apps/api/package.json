{
  "name": "@repo/api",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun build ./src/api.ts --outdir ./dist --target bun",
    "check-types": "tsc --noEmit",
    "dev": "bun run ./src/api.ts",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "start": "bun run ./dist/api.js",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@effect/platform-bun": "^0.79.0",
    "effect": "^3.17.8"
  },
  "devDependencies": {
    "@effect/vitest": "^0.25.1",
    "@repo/domain": "*",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@repo/vitest-config": "*",
    "@types/node": "^22.15.3",
    "eslint": "^9.33.0",
    "tsx": "^4.20.4",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
