  "dependencies": {
    "@effect/cli": "^0.69.2",
    "@effect/platform-bun": "^0.79.0",
    "effect": "^3.17.8"
  },
  "devDependencies": {
    "@effect/vitest": "^0.25.1",
    "@repo/domain": "*",
    "@repo/sdk": "*",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@repo/vitest-config": "*",
    "@types/bun": "^1.2.21",
    "eslint": "^9.33.0",
    "tsx": "^4.20.4",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
