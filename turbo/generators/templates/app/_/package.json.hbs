  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun build ./src/app.ts --outdir ./dist --target bun",
    "check-types": "tsc --noEmit",
    "dev": "bun run ./src/app.ts",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
